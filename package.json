{
  "name": "ups-chan",
  "version": "0.5.0",
  "description": "UPS guild bot",
  "main": "dist/dist.js",
  "author": "Hebilicious",
  "scripts": {
    "dev": "webpack --watch --config webpack/webpack.dev.js",
    "debug": "webpack --watch --config webpack/webpack.debug.js",
    "lint": "prettier --write {bot,webpack}/**/**.js",
    "test": "mocha --require @babel/register --slow 0",
    "build": "webpack --config webpack/webpack.prod.js",
    "prod": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "discord.js": "^11.3.0",
    "erlpack": "hammerandchisel/erlpack",
    "gifencoder": "^1.1.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "rethinkdbdash": "^2.3.31",
    "sanitize-filename": "^1.6.1",
    "sugar": "^2.0.4",
    "uws": "^9.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "@babel/runtime": "^7.0.0-beta.39",
    "babel-loader": "8.0.0-beta.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "husky": "^0.15.0-rc.6",
    "mocha": "^5.0.0",
    "node-dev": "^3.1.3",
    "prettier": "^1.10.2",
    "source-map-support": "^0.5.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "webpack-shell-plugin": "^0.5.0",
    "webpack2-externals-plugin": "^1.0.0"
  }
}
